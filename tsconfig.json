{
  "compilerOptions": {
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": false,
    "sourceMap": true,
    "target": "es6",
    "module": "NodeNext",
    "moduleResolution": "nodenext",
    "lib": [ "es2022", "dom" ],
    "types": [ "jquery", "facebook-js-sdk", "node" ]
  },
  "include": [
    "Views/**/*",
    "node_modules/@corprio/aspnetcore-site/dist/js/**/*"
  ]
}